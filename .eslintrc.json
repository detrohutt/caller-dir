{
  "root": true,
  "parser": "typescript-eslint-parser",
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true,
    "jest/globals": true
  },
  "plugins": ["jest"],
  "extends": ["eslint:recommended"],
  "rules": {
    "no-console": "off"
  }
}
